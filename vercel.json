{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "skipBuildCache": true,
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/_not-found",
      "dest": "/404.html"
    },
    {
      "src": "/404",
      "dest": "/404.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
